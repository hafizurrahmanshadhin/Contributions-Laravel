<IfModule mod_rewrite.c>
    RewriteEngine On

    # Ensure that requests that are not pointing to /public are rewritten to public/
    RewriteCond %{REQUEST_URI} !^/public/
    RewriteCond %{REQUEST_URI} !^/index\.php
    RewriteRule ^(.*)$ /public/$1 [L]
</IfModule>

